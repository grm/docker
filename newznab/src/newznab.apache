    <VirtualHost *:80>
        <Directory /var/www/newznab/www/>
            Options FollowSymLinks
            AllowOverride All
            Order allow,deny
            allow from all
        </Directory>

        ServerAdmin admin@example.com
        ServerName example.com 
        ServerAlias www.example.com
        DocumentRoot /var/www/newznab/www
        LogLevel warn
        ServerSignature Off
    </VirtualHost>

